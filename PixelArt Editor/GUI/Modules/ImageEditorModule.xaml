<UserControl x:Class="PixelArt_Editor.GUI.Modules.ImageEditorModule"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PixelArt_Editor.GUI.Modules"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Canvas x:Name="Canvas" SizeChanged="Canvas_SizeChanged">
        <Image x:Name="Img_image" MouseDown="Img_image_MouseDown" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Canvas>
</UserControl>
